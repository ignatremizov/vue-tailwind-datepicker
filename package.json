{
  "name": "@ignatremizov/vue-tailwind-datepicker",
  "type": "module",
  "version": "1.7.4",
  "description": "A Vue 3 datepicker component using Tailwind CSS and Day.js",
  "author": {
    "name": "Ignat Remizov",
    "email": "ignat@ignatremizov.com",
    "url": "https://github.com/ignatremizov"
  },
  "contributors": [
    {
      "name": "Ignat Remizov",
      "url": "https://github.com/ignatremizov"
    },
    {
      "name": "Kenhyuwa",
      "url": "https://github.com/kenhyuwa"
    },
    {
      "name": "Alexandre Le Corre",
      "url": "https://github.com/elreco"
    }
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ignatremizov"
  },
  "homepage": "https://github.com/ignatremizov/vue-tailwind-datepicker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignatremizov/vue-tailwind-datepicker.git"
  },
  "bugs": {
    "url": "https://github.com/ignatremizov/vue-tailwind-datepicker/issues",
    "email": "ignat@ignatremizov.com"
  },
  "keywords": [
    "vue-tailwind-datepicker",
    "vue-datepicker",
    "vue-daterangepicker",
    "vue-daterange-picker",
    "tailwind-datepicker",
    "tailwind-daterange-picker"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/vue-tailwind-datepicker.js",
      "require": "./dist/vue-tailwind-datepicker.umd.cjs"
    }
  },
  "main": "./dist/vue-tailwind-datepicker.umd.cjs",
  "module": "./dist/vue-tailwind-datepicker.js",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest run",
    "test:shortcut-matrix": "vitest run --passWithNoTests tests/unit/shortcut-*.spec.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix",
    "format:check": "eslint .",
    "docs:screenshots": "bash scripts/generate-doc-screenshots.sh",
    "docs:videos": "bash scripts/generate-doc-wheel-videos.sh",
    "docs:dev": "npm --prefix docs run dev",
    "docs:build": "npm --prefix docs run build",
    "docs:serve": "npm --prefix docs run serve",
    "typecheck": "vue-tsc --noEmit",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": ":sparkles: Features",
                "hidden": false
              },
              {
                "type": "fix",
                "section": ":bug: Fixes",
                "hidden": false
              },
              {
                "type": "docs",
                "section": ":memo: Documentation",
                "hidden": false
              },
              {
                "type": "style",
                "section": ":barber: Code-style",
                "hidden": false
              },
              {
                "type": "refactor",
                "section": ":zap: Refactor",
                "hidden": false
              },
              {
                "type": "perf",
                "section": ":fast_forward: Performance",
                "hidden": false
              },
              {
                "type": "test",
                "section": ":white_check_mark: Tests",
                "hidden": false
              },
              {
                "type": "ci",
                "section": ":repeat: CI",
                "hidden": false
              },
              {
                "type": "chore",
                "section": ":repeat: Chore",
                "hidden": false
              }
            ]
          }
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": []
        }
      ]
    ]
  },
  "peerDependencies": {
    "dayjs": "^1.11.13",
    "tailwindcss": "^4.1.11",
    "vue": "^3.3.13"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "vite-plugin-css-injected-by-js": "^3.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@headlessui/vue": "https://github.com/ignatremizov/headlessui/releases/download/v1.7.24-iremizov.1/headlessui-vue-1.7.24-iremizov.1.tgz",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.4",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.8.10",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "axe-core": "^4.11.1",
    "dayjs": "^1.11.19",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.3",
    "eslint-plugin-import-newlines": "^1.4.1",
    "fs-extra": "^11.3.3",
    "husky": "^8.0.3",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.49",
    "semantic-release": "^25.0.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^5.0.0-beta.6",
    "vitest": "^4.0.18",
    "vue": "^3.5.28",
    "vue-tsc": "^3.2.4"
  },
  "overrides": {
    "js-beautify": "https://codeload.github.com/ignatremizov/js-beautify/tar.gz/e5bacd263883f5297e4fa2aaeb2186a3f5905299",
    "minimatch": "10.2.2"
  }
}
